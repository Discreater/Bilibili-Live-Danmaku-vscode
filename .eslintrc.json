{
  "extends": [
    "@antfu"
  ],
  "rules": {
    "brace-style": "off",
    "@typescript-eslint/brace-style": ["error", "1tbs", { "allowSingleLine": true }],
    "semi": "off",
    "@typescript-eslint/semi": ["error", "always"],
    "no-console": "off",
    "accessor-pairs": "off",
    "@typescript-eslint/no-unused-vars": ["error", { "argsIgnorePattern": "^_", "varsIgnorePattern": "^_" }]
  }
}